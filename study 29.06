int main()
{
    int a = 0;
    int b = 0;
    int result = 0;
    for (a=1; a<=9;a++)
    {
        for (b = 1; b<=a; b++)
        {
            result = a*b;
            printf("%d*%d=%2d ",b, a, result);
        }
        printf("\n");
    }
    return 0;
}
