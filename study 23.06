int main()
{
    int i = 0;
    int m = 0;
    int result = 1;
    int sum = 0;
    for (i = 1; i<=3; i++)
    {
        for (m = 1; m<=i; m++)
        {
            result = result*m;
        }
        sum = sum+result;
    }
    printf("Result is %d\n",sum);
    return 0;
}

